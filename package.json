{
  "name": "mediz.id",
  "type": "module",
  "scripts": {
    "migrate:diff": "prisma migrate diff --from-schema-datasource ./prisma/schema.prisma --to-schema-datamodel ./prisma/schema.prisma",
    "migrate:dev": "env-cmd -f ./.env prisma migrate dev",
    "migrate:staging": "env-cmd -f ./.env.staging prisma migrate deploy",
    "migrate:supabase": "env-cmd -f ./.env.supabase prisma migrate deploy",
    "make:seed": "tsx ./cmd/cmd.ts make:seed --name",
    "db:seed:dev": "env-cmd -f ./.env tsx ./src/bin/cmd.ts db:seed",
    "db:seed:staging": "env-cmd -f ./.env.staging tsx ./cmd/cmd.ts db:seed",
    "dev": "env-cmd -f ./.env vinxi dev",
    "build": "prisma generate && vinxi build",
    "start": "vinxi start"
  },
  "dependencies": {
    "@ark-ui/solid": "^5.9.2",
    "@corvu/drawer": "^0.2.4",
    "@corvu/otp-field": "^0.1.4",
    "@corvu/resizable": "^0.2.5",
    "@fontsource/inter": "^5.2.5",
    "@kobalte/core": "^0.13.9",
    "@modular-forms/solid": "^0.25.1",
    "@prisma/client": "^6.8.2",
    "@solid-primitives/storage": "^4.3.2",
    "@solidjs/meta": "^0.29.4",
    "@solidjs/router": "^0.15.0",
    "@solidjs/start": "^1.1.0",
    "@tanstack/solid-table": "^8.21.3",
    "@trpc/server": "^11.1.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk-solid": "^1.1.2",
    "dataloader": "^2.2.3",
    "embla-carousel-solid": "^8.6.0",
    "node-cache": "^5.1.2",
    "solid-js": "^1.9.5",
    "solid-sonner": "^0.2.8",
    "valibot": "^1.1.0",
    "vinxi": "^0.5.3",
    "zod": "^3.25.30"
  },
  "devDependencies": {
    "@decs/typeschema": "^0.12.2",
    "@deepkit/type": "^1.0.8",
    "@tailwindcss/vite": "^4.0.7",
    "@trpc/client": "^11.1.3",
    "bcryptjs": "^3.0.2",
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "dayjs": "^1.11.13",
    "env-cmd": "^10.1.0",
    "prisma": "^6.8.2",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.0.7",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.4"
  },
  "engines": {
    "node": ">=22"
  }
}
